{
  "name": "replit-production-finder",
  "version": "1.0.0",
  "description": "A modern React application for discovering and analyzing production-ready repositories from Replit and GitHub",
  "type": "module",
  "scripts": {
    "dev": "vite --host",
    "dev:backend": "python app.py",
    "start:all": "concurrently -k -n backend,frontend \"python app.py\" \"vite --host\"",
    "build": "tsc --noEmit && vite build",
    "preview": "vite preview",
    "lint": "echo 'Linting skipped'"
  },
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.15.1",
    "@mui/material": "^5.15.1",
    "@mui/x-charts": "^6.18.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "socket.io-client": "^4.7.4"
  },
  "devDependencies": {
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^8.55.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "typescript": "^5.2.2",
    "vite": "^7.1.8",
    "concurrently": "^9.1.0"
  }
}
